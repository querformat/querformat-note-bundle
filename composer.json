{
    "name": "querformat/note-bundle",
    "type": "contao-bundle",
    "description": "Note application",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Maria Engel",
            "homepage": "https://www.querformat.info/"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/querformat/querformat-note-bundle"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.1",
        "contao/core-bundle": "4.4.*",
        "symfony/framework-bundle": "^3.3",
        "querformat/querformat-note-bundle": "dev-master"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.6",
        "leofeyer/optimize-native-functions-fixer": "^1.1",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Querformat\\NoteBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Querformat\\NoteBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Querformat\\NoteBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/querformat/querformat-note-bundle",
        "source": "https://github.com/querformat/querformat-note-bundle"
    }
}
